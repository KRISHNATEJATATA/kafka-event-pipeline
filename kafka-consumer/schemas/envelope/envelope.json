{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Envelope",
  "type": "object",
  "properties": {
    "eventId": {
      "type": "string"
    },
    "eventType": {
      "type": "string"
    },
    "eventVersion": {
      "type": "string"
    },
    "eventTime": {
      "type": "string",
      "format": "date-time"
    },
    "payload": {
      "type": "object"
    }
  },
  "required": ["eventId", "eventType", "eventVersion", "eventTime", "payload"]
}
